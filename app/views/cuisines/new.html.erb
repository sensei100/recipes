<%= render "shared/page_title", title: "Create a new Cuisine" %>

<%= render "shared/errors", obj: @cuisine %>

<div class="row">
  <div class="well col-md-8 col-md-offset-2">
    <%= form_for @cuisine do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
    
      <%= f.submit "Create Cuisine", class: "btn btn-success" %>
    <% end %>
  </div>  
</div>